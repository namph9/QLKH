    <class>com.namph.models.TblImportDetail</class>
    <class>com.namph.models.Import</class>
    <class>.TblProducts</class>
    <class>com.namph.models.TblProductType</class>
    <class>com.namph.models.TblProducts</class>
    <class>com.namph.models.TblExportDetail</class>
    <class>com.namph.models.TblDaily</class>
    <class>com.namph.models.TblThu</class>
    <class>com.namph.models.ViewThuTien</class>
    <class>com.namph.models.TblExport</class>
    <class>com.namph.models.User</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/warehouse"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
